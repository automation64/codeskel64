#!/usr/bin/env bash

source bin/devcs-set || exit 1
source "$DEVCS_BIN/bashlib64.bash"

bl64_fs_rm_file "$DEVCS_BUILD_PROD"
bl64_fs_merge_files \
  '0755' \
  "$BL64_LIB_DEFAULT" \
  "$BL64_LIB_DEFAULT" \
  "$DEVCS_BUILD_PROD" \
  "$DEVCS_SRC/cli/codeskel64.header" \
  "$DEVCS_SRC/cli/bashlib64.bash" \
  "$DEVCS_SRC/cli/codeskel64.bash"
